{
    "rules": {
        "empty-line-between-blocks": {
            "include": 0
        },
        "indentation": 4,
        "nesting-depth": 0,
        "no-trailing-whitespace": 0,
        "mixins-before-declarations": {
            "exclude": ["media-breakpoint-up", "media-breakpoint-down", "media-breakpoint-between", "media-breakpoint-only"]
        },
        "placeholder-in-extend": 0
    }
}
